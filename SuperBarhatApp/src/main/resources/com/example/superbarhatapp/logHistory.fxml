<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="624.0"
            xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
            fx:controller="com.example.superbarhatapp.LogHistoryController">
    <children>
        <!-- Таблица в которую мы будем выводит наш pojo -->
        <TableView fx:id="tableUsers" layoutX="12.0" layoutY="62.0" prefHeight="324.0" prefWidth="600.0">
            <columns>
                <!-- Колонки таблицы -->
                <TableColumn prefWidth="100.0" text="Должность" fx:id="postColumn" />
                <TableColumn prefWidth="150.0" text="ФИО" fx:id="fioColumn" />
                <TableColumn prefWidth="100.0" text="Логин" fx:id="loginColumn" />
                <TableColumn prefWidth="100.0" text="Пароль" fx:id="passwordColumn" />
                <TableColumn prefWidth="130.0" text="Последний вход" fx:id="lastLogColumn" />
                <TableColumn prefWidth="20.0" text="Тип входа" fx:id="typeLogColumn" />
            </columns>
        </TableView>
        <Label layoutX="14.0" layoutY="18.0" text="List all users:">
            <font>
                <Font name="Verdana" size="24.0" />
            </font>
        </Label>
    </children>
</AnchorPane>